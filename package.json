{
  "name": "hitext-prismjs",
  "version": "1.0.2",
  "description": "Prism.js adapter for HiText",
  "keywords": [
    "text",
    "code",
    "decoration",
    "highlight",
    "hitext",
    "prism",
    "prismjs",
    "source"
  ],
  "license": "MIT",
  "repository": "hitext/prismjs",
  "homepage": "https://github.com/hitext/prismjs",
  "bugs": {
    "url": "https://github.com/hitext/prismjs/issues"
  },
  "author": "Roman Dvornov <rdvornov@gmail.com> (https://github.com/lahmatiy)",
  "maintainers": [
    {
      "name": "Roman Dvornov",
      "email": "rdvornov@gmail.com",
      "github-username": "lahmatiy"
    }
  ],
  "main": "./src",
  "scripts": {
    "lint": "eslint src test",
    "lint-and-test": "npm run lint && npm test",
    "test": "mocha",
    "build": "browserify --no-bundle-external --standalone hitextPrismjs src/index.js > dist/hitext-prismjs.js && uglifyjs dist/hitext-prismjs.js --compress --mangle -o dist/hitext-prismjs.min.js",
    "prepublish": "npm run build",
    "coverage": "istanbul cover _mocha -- -R min",
    "travis": "npm run lint-and-test && npm run coveralls",
    "coveralls": "istanbul cover _mocha --report lcovonly -- -R min && cat ./coverage/lcov.info | coveralls"
  },
  "peerDependencies": {
    "prismjs": "^1.15.0"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "hitext": "^1.0.0-alpha.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "prismjs": "^1.15.0",
    "uglify-es": "^3.3.9"
  },
  "files": [
    "dist",
    "src",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ]
}
